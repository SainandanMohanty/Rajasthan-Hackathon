    
 <h2 class="page-header">Dashboard</h2>
<div class="container">
  <ul class="nav nav-pills">
    <li class="active"><a data-toggle="pill" href="#home">Patient Info</a></li>
    <li><a data-toggle="pill" href="#menu2">Health Records</a></li>
    <li><a data-toggle="pill" href="#menu3"><kbd>HealthCoin Account</kbd></a></li>
  </ul>
  
  <div class="tab-content">
    <div id="home" class="tab-pane fade in active">
      <h3>Basic Info</h3>
      
<!--here goes html that outputs patient info -->
   <section class="get">
    <a href="/get-data">LOAD DATA</a>
    <div>
        {{# each items }}
            <article class="item">
                <div>Name: {{ this.name }}</div>
                <div>email: {{ this.email }}</div>
                <div>address: {{ this.address }}</div>
                <div>mobile: {{ this.mobile }}</div>
                <div>aadharid: {{ this.aadharid }}</div>

            </article>
        {{/each}}
    </div>
    </section>
        <!--end of Patient Basic Info -->
        
    </div>
    <div id="menu2" class="tab-pane fade">
      <h3>Your Health Records</h3>

      <section class="get">
    <a href="/get-healthrecords">LOAD DATA</a>
    <div>
        {{# each items }}
            <article class="item">
                <div>Hospital name: {{ this.hospital }}</div>
                <div>complaint: {{ this.complaint }}</div>
                <div>diagnosis: {{ this.diagnosis }}</div>

            </article>
        {{/each}}
    </div>
    </section>
    </div>


    <div id="menu3" class="tab-pane fade"> 
    <h3>Hello</h3>
      <h4 id="name">{{name}}</h4>
      <h4 id="balance"></h4>
      <a href= "/get-healthcoins">REFRESH</a>
    </div>
  </div>
</div>

<script src="/js/web3.min.js"></script>
